{
    "name": "rest-express",
    "version": "1.0.0",
    "license": "MIT",
    "scripts": {
        "dev": "ts-node src/index.ts",
        "start": "npm run build && npm run serve",
        "serve": "node dist/src/index.js",
        "start-worker": "node dist/worker/index.js",
        "dev-worker": "npx ts-node worker/index.ts",
        "build": "npm run clean && npm run build-ts",
        "clean": "rm -rf dist",
        "build-ts": "tsc",
        "watch-ts": "tsc -w"
    },
    "dependencies": {
        "@mhoc/axios-digest-auth": "^0.8.0",
        "@prisma/client": "3.4.2",
        "axios": "^0.24.0",
        "cors": "^2.8.5",
        "cron": "^1.8.2",
        "dotenv": "^10.0.0",
        "express": "4.17.1",
        "typescript": "^4.4.4"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/cron": "^1.7.3",
        "@types/express": "4.17.13",
        "@types/node": "14.17.33",
        "prisma": "^3.4.2",
        "ts-node": "10.4.0"
    },
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    }
}
